<h1 class="page_title">Stuff I've Built</h1> 

<div id="posts_wrapper" class="skinny_wrapper">
    <% @post.each do |post| %>
        <div class="post">
            <p class="date"><%=post.created_at.strftime('%A, %B, %d')%></p>
            <h2>
                <a href="/posts/<%=post.id%>"><%=post.title%></a>
            </h2>
            <hr>
            </div> 
    <% end %> 
</div>
<%= will_paginate @post %>



